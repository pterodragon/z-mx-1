include "version.fbs";
include "link.fbs";
namespace ZDash.fbs;
union ReqAckData {
  VersionAck	: Version,
  MkLinkAck	: Link,
  RmLinkAck	: Link,
  ConnectAck	: Link,
  DisconnectAck	: Link,
  LinksAck	: LinkList,
  SelectAck	: Link
}
table ReqAck {
  seqNo		:uint64;	// request ID (restarts at 0 with each session)
  data		:ReqAckData;
  rejCode	:uint32 = 0;
  rejText	:string;
}
root_type ReqAck;
